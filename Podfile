# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'HMBaseProj' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

    pod 'Alamofire'  #网络请求
    pod 'SwiftyJSON' #json解析
    pod 'SnapKit'   #auto Layout
    pod 'Eureka'    #表单
    pod 'SwiftyUserDefaults'
    pod 'IQKeyboardManagerSwift',:inhibit_warnings => true #键盘

    pod 'RxSwift'  #RxSwift
    pod 'RxCocoa'  #RxSwift

    pod 'DNSPageView' #
    pod 'SQLite.swift' #sql
    pod 'Hero'  #转场动画
    pod 'CryptoSwift'  #加密
    pod 'ESTabBarController-swift'  #Tabbar
    pod 'Kingfisher'  #图片缓存
    pod 'MJRefresh',:inhibit_warnings => true #下拉刷新
    pod 'Fusuma' #相机、图片










  # Pods for HMBaseProj

  target 'HMBaseProjTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HMBaseProjUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
